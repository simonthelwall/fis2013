\ProvidesPackage{beamercolorthemewhale_PHE}


\mode<presentation>

\definecolor{comp_blue}{cmyk}{1, 0.94, 0, 0.47}

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=comp_blue}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=comp_blue}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=comp_blue}
\setbeamercolor*{palette quaternary}{fg=white,bg=comp_blue}

\setbeamercolor*{sidebar}{use=structure,bg=comp_blue}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
