\ProvidesPackage{beamercolorthemeorchid_PHE}


\mode<presentation>

\definecolor{comp_blue}{cmyk}{1, 0.94, 0, 0.47}

\setbeamercolor{block title}{use=structure,fg=white,bg=comp_blue}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!comp_blue}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!comp_blue}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\mode
<all>
